{
  "name": "@goingmerry/squarespace-multilingual-navigation",
  "version": "1.0.3",
  "source": "src/index.ts",
  "browser": "dist/main.js",
  "files": ["dist/main.js","dist/main.js.map"],
  "description": "javascript package to enable multilingual navigation on squarespace",
  "repository": {
    "type": "git",
    "url": "https://github.com/goingmerry-ch/squarespace-multilingual-navigation.git"
  },
  "license": "MIT",
  "targets": {
    "browser": {
      "context": "browser",
       "includeNodeModules": true,
       "isLibrary": true
    }
  },
  "type": "module",
  "scripts": {
    "serve": "parcel serve demo/layout_right/index.html",
    "watch": "parcel watch",
    "build": "parcel build --no-cache",
    "publish": "npm run build && npm publish --access public"
  },
  "devDependencies": {
    "parcel": "latest"
  },
  "dependencies": {
    "lit": "^3.1.0",
    "lit-html": "^3.1.0"
  }
}
